# .env.example
# Copy this to .env and fill in your values

# Database (Neon DB PostgreSQL)
# Format: postgresql://username:password@ep-xxx.region.neon.tech/farming_db
# Get this from your Neon DB dashboard
DATABASE_URL=postgresql://user:password@ep-xxx.region.neon.tech/farming_db

# OpenWeather API
# Get your free API key from: https://openweathermap.org/api
OPENWEATHER_API_KEY=your_api_key_here

# Application
DEBUG=False
SECRET_KEY=your-secret-key-change-in-production

# ML Models
ML_MODEL_PATH=ml_models/crop_yield_model.pkl
